{
    "name": "clarkwinkelmann/flarum-ext-lock-likes",
    "description": "Remove ability to like posts in a locked discussion",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.0",
        "flarum/likes": "*",
        "flarum/lock": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-lock-likes",
        "forum": "https://discuss.flarum.org/d/23078"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\LockLikes\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Lock Likes",
            "category": "feature",
            "icon": {
                "name": "fas fa-lock",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/23078"
        }
    }
}
